
  {{#if isError}}
    You are not authorized to view problems of this contest.
  {{else}}
  <div class="border-card mt-4">
    <div class="list-divided border-none">
      {{#each contents as |content|}}      
        <ContestViews::ContestDefaultView::ContentListItem 
          @solveButtonClass={{solveButtonClass}}
          @route={{route}} 
          @content={{content}}
          @contest={{contest}} />
      {{/each}}
    </div>
  </div>
    {{yield contents}}
  {{/if}}