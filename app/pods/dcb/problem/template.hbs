<div class="row font-xs bold mx-0">
  <BreadCrumbs
    @outputStyle="foundation"
    @tagName="ul"
    @linkable={{true}}
    @linkClass="orange"
    @crumbClass="px-3" />
</div>
<div class="row">
  <div class="col-8">
    <DcbProblemCard
      class="pb-4"
      @problem={{problem}} />
    <ProblemView 
      class="py-4"
      @contest={{contest}}
      @problem={{problem}} />
  </div>
  <div class="col-4">
    <DailyGoalCardSmall 
      class="py-4"
      @level={{level}} />
    <RatingCard 
      class="py-4"
      @contest={{contest}}
      @problem={{problem}} />
    <AuthorCard class="py-4" />
  </div>
</div>
