<div class="row px-5">
  <div class="col-8">
    <DcbProblemCard
      class="py-4"
      @problem={{problem}} />
    <ProblemView 
      class="py-4"
      @problem={{problem}} />
    <CodeEditorComponent 
      class="py-4"
      @onRunTask={{onRunTask}} 
      @onSubmitTask={{onSubmitTask}} />
