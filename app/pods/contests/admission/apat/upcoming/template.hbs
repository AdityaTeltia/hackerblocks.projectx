<div class="pt-5">
  {{#if (not admission_contests.length)}}
    {{contest-view/empty-contest-screen}}
  {{/if}}
  <div class="row">
    {{#each admission_contests as |contest|}}
    <div class="col-12 col-md-4 mb-4">
      <ContestCard
        @contest={{contest.contest}}
        @showTimer=true >
        <RegisterButton 
          @contestTypeModel={{contest}} 
          @route='contests.contest' />
      </ContestCard>
    </div>
    {{/each}}
  </div>
</div>
