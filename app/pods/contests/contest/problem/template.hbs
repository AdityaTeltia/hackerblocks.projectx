<div class="d-flex font-xs bold mb-4 mt-1">
  <BreadCrumbs
    @outputStyle="foundation"
    @tagName="ul"
    @linkable={{true}}
    @crumbClass="px-3" />
</div>
<div class="row my-4">
  <div class="col-12">
    <PracticeProblemCard @problem={{content}} />
  </div>
</div>

<div class="row">
  <div class="col-12 col-md-8">
    <ProblemView @contest={{contest}} @problem={{content}} />
  </div>
  <div class="col-12 col-md-4">
    {{!-- {{#if content.canSubmitReview}}
    <RatingCard class="py-4" @contest={{contest}} @problem={{content}} />
    {{/if}} --}}
    <AuthorCard class="py-4" @author={{content.createdAt}} />
  </div>
</div>
