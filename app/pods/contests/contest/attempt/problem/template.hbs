<FullScreenContestView
  @route="contests.contest.attempt"
  @attempt={{contest_attempt}}
  @onTimerEnd={{onTimerEnd}}
  @submitTask={{submitTask}}
  @contest={{contest}}>
  <div class="col-11 offset-1">
    <FullScreenProblemView
      @problem={{problem}}
      @contest={{contest}} />
  </div>
</FullScreenContestView>
