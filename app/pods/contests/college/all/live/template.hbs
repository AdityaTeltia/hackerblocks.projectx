<div class="row">
  {{#each college_contests as |college_contest|}}
  <div class="col-12 col-md-4">
    <ContestCard 
      @contest={{college_contest.contest}}
      @showTimer=true>
        {{#link-to "contests.college.contest" college_contest.id class="button-solid button-orange mt-3 mb-2 mx-auto" tagName="button"}}
          Solve Challenge
        {{/link-to}}
    </ContestCard>
  </div>
  {{/each}}
</div>