<div class="row">
  {{#each contests as |contest|}}
  <div class="col-12 col-md-4">
    <CollegeContestCard @collegeContest={{contest}}>
      {{#link-to "contests.college.id" contest.id class="button-solid button-orange mt-3 mb-2 mx-auto" tagName="button"}}
        Solve Challenge
      {{/link-to}}
    </CollegeContestCard>
  </div>
  {{/each}}
</div>