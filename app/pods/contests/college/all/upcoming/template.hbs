<div class="row">
  {{#each college_contests as |college_contest|}}
  <div class="col-12 col-md-4">
    <ContestCard 
      @contest={{college_contest.contest}}
      @showTimer=true>
        <button class="button-solid button-orange mt-3 mb-2 mx-auto" 
          {{action (perform createAttemptTask contest)}}>
          Register
        </button>
    </ContestCard>
  </div>
  {{/each}}
</div>