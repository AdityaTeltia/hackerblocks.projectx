{{#if (not college_contests.length)}}
  {{contest-view/empty-contest-screen}}
{{/if}}
<div class="row">
  {{#each college_contests as |college_contest|}}
  <div class="col-12 col-md-4">
    <ContestCard class="mb-5"
      @contest={{college_contest.contest}}
      @showTimer=true>
        <RegisterButton 
          @contestTypeModel={{college_contest}}
          @route="contests.contest" />
    </ContestCard>
  </div>
  {{/each}}
</div>