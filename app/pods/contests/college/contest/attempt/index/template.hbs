<ContestView
  @contest={{contest}}
  @page={{page}}
  @status={{status}}
  @difficulty={{difficulty}}
  @contentTypeId={{college_contest.id}}
  @problemListRoute="contests.college.contest.attempt.problem"
  @setOffset={{action 'setOffset'}} as |contestView|>
  <PracticeFilterComponent
    @status={{status}}
    @changeStatusFilter={{action (mut status)}}
    @difficulty={{difficulty}}
    @changeDifficultyFilter={{action (mut difficulty)}} />
  <div class="border-card mt-5">
    {{contestView.leaderboardComponent}}
  </div>
</ContestView>
